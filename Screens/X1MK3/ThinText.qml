import QtQuick 2.0

Text {
    font {
        family: "TRAKTORFREON"
        weight: 50
        pixelSize: 36
        letterSpacing: 0
        preferShaping: false
        kerning: false
        capitalization: Font.Capitalize
        hintingPreference: Font.PreferFullHinting
    }
    color: "white"
    antialiasing: false
    renderType: Text.NativeRendering
    verticalAlignment: Text.AlignVCenter
    horizontalAlignment: Text.AlignLeft
}